modify_collection <- function(collection) {
collection[2] = "blue"
return(collection)
}
collection1 = c(1, 2, 3, 4, 5)
collection2 = modify_collection(collection1)
print(collection1) # [1] 1 2 3 4 5
print(collection2) # "1"    "blue" "3"    "4"    "5"
